{
  "private": true,
  "name": "upload-release-assets",
  "version": "1.0.0",
  "description": "Upload assets to a release",
  "source": "index.js",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "prepare": "ncc build index.js -o dist --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run lint && npm run prepare && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/npetzall/upload-release-assets.git"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/npetzall/upload-release-assets/issues"
  },
  "homepage": "https://github.com/npetzall/upload-release-assets#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "jest": "^29.4.3",
    "mock-fs": "^5.2.0",
    "nock": "^13.3.0",
    "prettier": "^2.8.7"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/upload-release-asset.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
